<!-- Meta -->
<meta property="twitter:card" content="summary_large_image" />
<meta property="og:site_name" content="Bitquery Explorer" />
<meta property="vk:image" content="<%= image_path 'bitqueryLogoicon.png' %>" />
<meta property="twitter:image" content="<%= image_path 'bitqueryLogoicon.png' %>" />
<meta property="og:image" content="<%= image_path 'bitqueryLogoicon.png' %>" />
<meta property="og:image:width" content="720" />
<meta property="og:image:height" content="378" />
<meta property="og:title" content="<%= content_for?(:title) ? yield(:title) : 'Bitquery Blockchain Explorer' %>" />
<meta property="og:type" content="article" />
<meta property="og:description" content="<%= content_for?(:description) ? yield(:description) : 'Bitquery is analytical engine for blockchains' %>" />
<meta property="og:url" content="<%= request.original_url %>" />

<link rel="image_src" href="<%= image_path 'bitqueryLogoicon.png' %>">
<meta name="twitter:card" content="summary_large_image" />

<link rel="apple-touch-icon" href="<%= image_path 'bitqueryLogoicon144x144.png' %>">
<meta name="msapplication-tile-image" content="<%= image_path 'bitqueryLogoicon.png' %>">

<!-- Chrome, Firefox OS and Opera -->
<meta name="theme-color" content="#12263f" />
<!-- Windows Phone -->
<meta name="msapplication-navbutton-color" content="#12263f" />
<!-- iOS Safari -->
<meta name="apple-mobile-web-app-status-bar-style" content="#12263f" />
